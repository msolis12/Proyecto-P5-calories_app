package com.example.appcalorica.model

data class UserModel(
    val id: String = "",
    val name: String = "",
    val sex: String = "",
    val age: Int = 0,
    val heightCm: Int = 0,
    val weightKg: Float = 0f,
    val goalCalories: Int = 2000,
    val goalType: String = "Mantenimiento",
    val activityLevel: String = "Sedentario",
    val weightHistory: List<WeightEntry> = listOf() // Se usa para StatsScreen
)

